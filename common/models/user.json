{
  "name": "user",
  "base": "User",
  "idInjection": false,
  "options": {
    "table": "User"
  },
  "hidden": [
    "id",
    "realm",
    "emailVerified"
  ],
  "protected":[
    
  ],
  
  "properties": {
    "email": {
      "type": "string",
      "id": true,
      "required": true,
      "mysql": {
        "columnName": "email",
        "dataType": "varchar",
        "dataLength": 50,
        "nullable": "N"
      }
    },
    "username": {
      "type": "string",
      "required": true,
      "mysql": {
        "columnName": "username",
        "dataType": "VARCHAR",
        "dataLength": 20,
        "nullable": "N"
      }
    },
    "regdate": {
      "type": "date",
      "require": true
    },
    "tel": {
      "type": "string",
      "mysql": {
        "columnName": "tel",
        "dataType": "VARCHAR",
        "dataLength": 20,
        "nullable": "Y"
      }
    },
    "password": {
      "type": "string",
      "required": true,
      "mysql": {
        "columnName": "tel",
        "dataType": "VARCHAR",
        "dataLength": 50,
        "nullable": "N"
      }
    }
  },
  "mixins": {
    "ValidateProperty": true
  },
  "validations": [],
  "relations": {
    "carriers": {
      "type": "hasMany",
      "model": "carrier",
      "foreignKey": "userEmail",
      "through": "CarrierMapping"
    },
    "deliveryMapping": {
      "type": "hasMany",
      "model": "DeliveryMapping",
      "foreignKey": "userEmail"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    },
    "roleMapping": {
      "type": "hasMany",
      "model": "RoleMapping",
      "foreignKey": "principalId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "getDelivery"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "getDelivery"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "getAllDelivery"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "getAllDelivery"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
}
