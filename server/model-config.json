{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false,
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "user",
        "foreignKey": "userEmail"
      }
    }
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "carrier": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": true,
          "find": true,
          "findById": true,
          "prototype.__get__users": true
        }
      }
    }
  },
  "user": {
    "dataSource": "db",
    "public": true,
    "options": {
      "emailVerificationRequired": true,
      "remoting": {
        "sharedMethods": {
          "*": true,
          "create": true,
          "find": true,
          "findById": true,
          "deleteById": true,
          "changePassword": true,
          "login": true,
          "prototype.patchAttributes": true,
          "prototype.verify": true,
          "confirm": true,
          "resetPassword": true
        }
      }
    }
  },
  "CarrierMapping": {
    "dataSource": "db",
    "public": false
  },
  "delivery": {
    "dataSource": "db",
    "public": false
  },
  "Email": {
    "dataSource": "Email"
  }
}
